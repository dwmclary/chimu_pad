<%= javascript_include_tag :defaults %>
<%= csrf_meta_tag %>

<script type="text/javascript">

function BUDEffect(element){
    new Effect.toggle(element,'Blind', {duration:0.5});
}
</script>

<h3>
<div onclick="BUDEffect($(roster))">
    Roster
</div>    

</h3>

<div id = "roster">
    <%=render "roster"%>
</div>

<h3>
<div onclick="BUDEffect($(matches))">
   Matches
</div>
</h3>
<div id = "matches">
    <%= render "match_list" %>
</div>